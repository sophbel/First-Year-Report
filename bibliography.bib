@article{nelsonCapsuleEnhancesPneumococcal2007,
  title = {Capsule {{Enhances Pneumococcal Colonization}} by {{Limiting Mucus}}-{{Mediated Clearance}}},
  author = {Nelson, Aaron L. and Roche, Aoife M. and Gould, Jane M. and Chim, Kannie and Ratner, Adam J. and Weiser, Jeffrey N.},
  date = {2007-01-01},
  journaltitle = {Infection and Immunity},
  volume = {75},
