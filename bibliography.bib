@article{cantatoreAngiogenesisDysregulationPsoriatic2017,
  title = {Angiogenesis {{Dysregulation}} in {{Psoriatic Arthritis}}: {{Molecular Mechanisms}}},
  author = {Cantatore, Francesco Paolo and Maruotti, Nicola and Corrado, Addolorata and Ribatti, Domenico},
  date = {2017-07},
  journaltitle = {BioMed Research International},
