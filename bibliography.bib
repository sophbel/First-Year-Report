}

@article{lundbergHomogeneousAntibodybasedProximity2011,
  title = {Homogeneous Antibody-Based Proximity Extension Assays Provide Sensitive and Specific Detection of Low-Abundant Proteins in Human Blood.},
  author = {Lundberg, Martin and Eriksson, Anna and Tran, Bonnie and Assarsson, Erika and Fredriksson, Simon},
