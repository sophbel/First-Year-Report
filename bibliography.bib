@article{cookWholeanimalConnectomesBoth2019,
  title = {Whole-Animal Connectomes of Both {{Caenorhabditis}} Elegans Sexes},
  author = {Cook, Steven J. and Jarrell, Travis A. and Brittin, Christopher A. and Wang, Yi and Bloniarz, Adam E. and Yakovlev, Maksim A. and Nguyen, Ken C. Q. and Tang, Leo T.-H. and Bayer, Emily A. and Duerr, Janet S. and Bülow, Hannes E. and Hobert, Oliver and Hall, David H. and Emmons, Scott W.},
