@article{guzmanNeutralizingAntibodiesInfection2007,
  title = {Neutralizing Antibodies after Infection with Dengue 1 Virus.},
  author = {Guzman, Maria G and Alvarez, Mayling and Rodriguez-Roche, Rosmari and Bernardo, Lidice and Montes, Tibaire and Vazquez, Susana and Morier, Luis and Alvarez, Angel and Gould, Ernest A and Kouri, Gustavo and Halstead, Scott B},
