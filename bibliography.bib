@article{suarez-farinasExpandingPsoriasisDisease2012,
  title = {Expanding the {{Psoriasis Disease Profile}}: {{Interrogation}} of the {{Skin}} and {{Serum}} of {{Patients}} with {{Moderate}}-to-{{Severe Psoriasis}}},
  author = {Suárez-Fariñas, Mayte and Li, Katherine and Fuentes-Duculan, Judilyn and Hayden, Karen and Brodmerkel, Carrie and Krueger, James G.},
