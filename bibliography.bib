@article{oggioniPneumococcalZincMetalloproteinase2003,
  title = {Pneumococcal Zinc Metalloproteinase {{ZmpC}} Cleaves Human Matrix Metalloproteinase 9 and Is a Virulence Factor in Experimental Pneumonia},
  author = {Oggioni, Marco R. and Memmi, Guido and Maggi, Tiziana and Chiavolini, Damiana and Iannelli, Francesco and Pozzi, Gianni},
