@article{gladmanPsoriaticArthritisEpidemiology2005,
  title = {Psoriatic Arthritis: Epidemiology, Clinical Features, Course, and Outcome},
  shorttitle = {Psoriatic Arthritis},
  author = {Gladman, D. D. and Antoni, C. and Mease, P. and Clegg, D. O. and Nash, P.},
