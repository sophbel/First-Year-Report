@article{krakowskiPsoriaticArthritisNew2019,
  title = {Psoriatic Arthritis - New Perspectives.},
  author = {Krakowski, Przemys\textbackslash law and Gerkowicz, Agnieszka and Pietrzak, Aldona and Krasowska, Dorota and Jurkiewicz, Andrzej and Gorzelak, Mieczys\textbackslash law and Schwartz, Robert A},
